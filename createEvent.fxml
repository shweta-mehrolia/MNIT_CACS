            <Label layoutX="42.0" layoutY="65.0" text="ENTER EVENT NAME:">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <TextField fx:id="ename" layoutX="168.0" layoutY="61.0" />
            <TextArea fx:id="desc" layoutX="42.0" layoutY="140.0" prefHeight="200.0" prefWidth="426.0" />
            <Button fx:id="create" layoutX="373.0" layoutY="356.0" mnemonicParsing="false" onMouseClicked="#create" text="CREATE EVENT" />
         </children>
      </AnchorPane>
   </center>
